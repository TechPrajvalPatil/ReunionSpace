@model IEnumerable<Feeds>
<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h3 class="fw-bolder">Accepted Feeds</h3>
            <table class="table table-striped text-center">
                <thead>
                    <tr>
                        <th class="fw-bolder"> # </th>
                        <th class="fw-bolder">FeedTitle</th>
                        <th class="fw-bolder">Feeds Date</th>
                        <th class="fw-bolder">FeedsType</th>
                        <th class="fw-bolder">Actions</th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        int srno = 1;
                    }
                    @foreach (var temp in Model)
                    {
                        <tr>
                            <td>@srno</td>
                            <td>@temp.FeedsTitle</td>
                            <td>@temp.FeedsDate.ToString("MMMM dd, yyyy")</td>
                            <td>@temp.FeedType</td>
                            <td>
                                <a asp-action="Details" asp-controller="ManageFeeds" asp-area="CompanyAdminArea" asp-route-id="@temp.FeedsId" class="btn btn-success btn-rounded btn-fw">Details</a>
                            </td>
                        </tr>
                        srno++;
                    }

                </tbody>
            </table>
        </div>
    </div>
</div>
